{
  parser: "babel-eslint",
  extends: [
    "airbnb"
  ],
  plugins: [
    "eslint-plugin-react",
    "babel"
  ],
  rules: {
    "react/forbid-prop-types": 0,
    "react/no-find-dom-node": 0,
    "react/jsx-filename-extension": 0,
    "arrow-parens": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "no-var": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0
  }
}
